{
  "name": "@wcm/boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate module",
  "main": "dist/bundle.cjs.js",
  "module": "dist/bundle.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "test": "NODE_ENV=test APP=test ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha"
  },
  "publishConfig": {
    "@wcm:registry": "https://nexusrepo.antwerpen.be/repository/npm-private"
  },
  "keywords": [
    "Boilerplate",
    "WCM",
    "Pelorus",
    "module",
    "CMS"
  ],
  "wcmModule": {
    "angularDirPath": "public/app/",
    "cssDirPath": "public/styles/",
    "moduleConfig": {
      "angularModule": "wcm-boilerplate",
      "translationKey": "External module",
      "operationType": "boilerplate",
      "icon": "ban",
      "type": "system",
      "machineName": "external-module_0.0.1",
      "navigationItem": true,
      "authentication": {
        "requiresLogin": true
      }
    },
    "variables": []
  },
  "author": "Jeroen Valcke",
  "license": "ISC",
  "dependencies": {
    "@wcm/module-helper": "1.0.0",
    "glob": "^7.1.2",
    "path": "^0.12.7",
    "q": "^1.5.1",
    "ramda": "^0.25.0",
    "rootpath": "~0.1.2"
  },
  "devDependencies": {
    "@district01/eslint-config": "1.0.0",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@types/react": "^16.9.17",
    "apidoc-swagger": "git+https://github.com/JvJefke/apidoc-swagger.git",
    "chai": "~4.1.2",
    "istanbul": "^0.4.5",
    "mocha": "~4.0.1",
    "react": "^16.12.0",
    "rollup": "^1.29.0",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "sinon": "~4.0.2",
    "typescript": "^3.7.4"
  }
}
